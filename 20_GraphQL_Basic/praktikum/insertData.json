"Comment: Mutation for Insert Data"

mutation InsertData($object: kampus_merdeka_keterangan_insert_input!) {
    insert_kampus_merdeka_keterangan_one(object: $object) {
        id
        id_anggota
        pelajaran
        nilai
        status
    }
}


